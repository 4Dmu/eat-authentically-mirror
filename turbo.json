{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalDependencies": [".env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
        "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
        "NEXT_PUBLIC_CLERK_SIGN_UP_FALLBACK_REDIRECT_URL",
        "NEXT_PUBLIC_CLERK_SIGN_IN_FALLBACK_REDIRECT_URL",
        "CLERK_SIGNING_SECRET",
        "STRIPE_SECRET_KEY",
        "STRIPE_SIGNING_SECRET",
        "NEXT_PUBLIC_GOOGLE_MAPS_JS_PUBLIC_KEY",
        "GOOGLE_MAPS_API_KEY",
        "TOTAL_GEOCODE_REQUESTS_ALLOWED",
        "SITE_URL",
        "COMMUNITY_MEMBER_MONTHLY_PRICE_ID",
        "COMMUNITY_MEMBER_YEARLY_PRICE_ID",
        "ORGANIZATION_PRO_MONTHLY_PRICE_ID",
        "ORGANIZATION_PRO_YEARLY_PRICE_ID",
        "ORGANIZATION_PREMIUM_MONTHLY_PRICE_ID",
        "ORGANIZATION_PREMIUM_YEARLY_PRICE_ID",
        "PRODUCER_ENTERPRISE_MONTHLY_PRICE_ID",
        "PRODUCER_ENTERPRISE_YEARLY_PRICE_ID",
        "TURSO_DB_URL",
        "TURSO_DB_TOKEN",
        "SAFE_CLOUDFLARE_API_TOKEN",
        "SAFE_CLOUDFLARE_ACCOUNT_ID",
        "SAFE_CLOUDFLARE_ACCOUNT_HASH",
        "UPSTASH_REDIS_REST_URL",
        "UPSTASH_REDIS_REST_TOKEN",
        "STREAM_WEBHOOK_SECRET",
        "RESEND_API_KEY",
        "RESEND_FROM_EMAIL",
        "VERCEL_ENV",
        "TWILIO_ACCOUNT_SID",
        "TWILIO_AUTH_TOKEN",
        "TWILIO_NUMBER",
        "RESEND_WAITLIST_AUDIENCE_ID",
        "NEXT_PUBLIC_TURNSTILE_SITE_KEY",
        "TURNSTILE_SECRET_KEY",
        "NEXT_PUBLIC_AXIOM_TOKEN",
        "NEXT_PUBLIC_AXIOM_DATASET",
        "OPENAI_API_KEY",
        "BETTER_AUTH_SECRET",
        "BETTER_AUTH_URL",
        "ADMIN_TURSO_DB_URL",
        "ADMIN_TURSO_DB_TOKEN",
        "AUTH_PASSKEY_RP_ID",
        "AUTH_PASSKEY_RP_NAME",
        "AUTH_PASSKEY_ORIGIN",
        "NEXT_PUBLIC_TYPESENSE_HOST",
        "NEXT_PUBLIC_TYPESENSE_PORT",
        "NEXT_PUBLIC_TYPESENSE_PROTOCOL",
        "TYPESENSE_APIKEY",
        "NEXT_PUBLIC_TYPESENSE_SEARCH_ONLY_APIKEY",
        "ORGANIC_CERT_ID"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
